journal = {Proceedings of the SIGCHI conference on Human Factors in Computing Systems},
mendeley-groups = {HCI/Classics},
number = {3},
pages = {50--53},
pmid = {5495722},
title = {{Designing for usability---key principles and what designers think}},
url = {http://portal.acm.org/citation.cfm?id=800045.801579{\&}coll=portal{\&}dl=ACM{\&}CFID=67429272{\&}CFTOKEN=32437027},
volume = {28},
year = {1983}
}

