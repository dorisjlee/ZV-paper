keywords = {E32 [Software and its Engineering],Genomics,H132 [Information Systems],Life and medical sciences— Computational biology,MapReduce,Scientific Computing,parallel and distributed DBMSs},
mendeley-groups = {Big Data Analytics},
pages = {631--646},
